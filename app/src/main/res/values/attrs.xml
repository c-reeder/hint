<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DiagonalDoubleTextView">
        <attr name="text1" format="string" />
        <attr name="text2" format="string" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TenSpinner">
        <attr name="diameter" format="dimension" />
        <attr name="horizontalGravity" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="verticalGravity" format="enum">
            <enum name="top" value="3" />
            <enum name="center" value="1" />
            <enum name="bottom" value="4" />
        </attr>
    </declare-styleable>
</resources>